{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*).(ico|svg|jpg|jpeg|png|webp|gif|css|js|json)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "@react_app_api_url"
    }
  }
}
